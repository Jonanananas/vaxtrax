<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vaxtrax">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.VaxTrax">
        <activity
            android:name=".VaccineDetailsActivity"
            android:exported="true" />
        <activity
            android:name=".CheckVaxActivity"
            android:exported="true" />
        <activity
            android:name=".RecommendVaccinesActivity"
            android:exported="true" />
        <activity
            android:name=".AddVaxActivity"
            android:exported="true"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".WelcomeActivity"
            android:exported="true" />
        <activity
            android:name=".TravelVaccinationsActivity"
            android:exported="true" />
        <activity
            android:name=".TravelguideActivity"
            android:exported="true" />
        <activity
        android:name=".VaccineInfoActivity"
        android:exported="true" />

    </application>

</manifest>